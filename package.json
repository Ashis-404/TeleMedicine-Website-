{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx src/server.ts",
    "dev:fullstack": "node start-fullstack-server.cjs",
    "dev:mock-api": "node mock-api-server.cjs",
    "dev:with-api": "concurrently \"npm run dev:mock-api\" \"npm run dev\"",
    "build": "vite build",
    "start": "vite preview",
    "vercel-build": "npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "demo": "cd demo && python app.py",
    "start-demo": "node start-demo.cjs",
    "start-combined": "node start-combined-server.cjs",
    "full-server": "npm run start-combined",
    "db:backup": "node scripts/database-backup.js",
    "db:init": "mysql -u root -p < tele-backend/migrations/enhanced_schema.sql",
    "db:verify": "node scripts/database-backup.js --verify-only"
  },
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/sql.js": "^1.4.9",
    "@vercel/node": "^3.0.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "mysql2": "^3.14.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.0",
    "sql.js": "^1.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.20"
  }
}
